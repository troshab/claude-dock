    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" notification || true",
            "async": true
          }
        ]
      }
    ],
    "TaskCompleted": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" task_completed --sync 30 --block || true"
