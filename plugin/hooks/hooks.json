{
  "description": "claude-dock daemon hooks: dock-send.sh -> daemon :19543 -> Tabby :19542",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" session_start --start-daemon || true",
            "async": true
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" user_prompt || true",
            "async": true
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" tool_start || true",
            "async": true
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" tool_end || true",
