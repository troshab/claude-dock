    "PermissionRequest": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" permission_request --sync 590 || true"
          }
        ]
      }
    ],
    "SubagentStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" subagent_start || true",
            "async": true
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" subagent_stop --sync 30 || true"
