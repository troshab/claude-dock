    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "test -n \"$CLAUDE_DOCK_SOURCE\" && bash \"${CLAUDE_PLUGIN_ROOT}/dock-send.sh\" session_end || true",
            "async": true
          }
        ]
      }
    ]
